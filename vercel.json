{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "editor.html",
      "use": "@vercel/static"
    },
    {
      "src": "preview.html",
      "use": "@vercel/static"
    },
    {
      "src": "editor.js",
      "use": "@vercel/static"
    },
    {
      "src": "editor.css",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(magx|editor)",
      "dest": "/editor.html"
    },
    {
      "src": "/preview",
      "dest": "/preview.html"
    },
    {
      "src": "/(editor\\.js|editor\\.css|editor\\.html|preview\\.html|favicon\\.ico|.*\\.(png|jpg|jpeg|svg|webp|gif))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
