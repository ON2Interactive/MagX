# Copy this file to .env and set your secrets.

# At least one provider key is required for /api/layout-generate.
ANTHROPIC_API_KEY=""
GOOGLE_API_KEY=""
# Optional alias if you prefer naming it GEMINI_API_KEY.
GEMINI_API_KEY=""
# Required for /api/image-generate (Recraft image generation + edit).
RECRAFT_API_TOKEN=""

# Optional model overrides.
# Defaults:
# - Anthropic: claude-opus-4-6
# - Google/Gemini: gemini-2.0-flash
ANTHROPIC_LAYOUT_MODEL=""
ANTHROPIC_MODEL=""
GOOGLE_LAYOUT_MODEL=""
GEMINI_LAYOUT_MODEL=""
# - Recraft image model defaults:
#   - generation: recraftv4 (1K), recraftv4_pro (2K/4K)
#   - image-to-image edit: recraftv3
RECRAFT_IMAGE_MODEL=""
RECRAFT_IMAGE_EDIT_MODEL=""

# Local server port.
PORT=4174
